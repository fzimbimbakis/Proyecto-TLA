Class Rectangulo{
    Attributes {
            int ladoA;
            int ladoB;
        }

        Constructor Rectangulo(int ladoA, int ladoB){
          this.ladoA = ladoA;
          this.ladoB = ladoB;
        }


    int area () {
           return this.ladoA * this.ladoB;
        }
    int perimetro(){
           return this.ladoA * 2 + this.ladoB * 2;
        }
}
Class Cuadrado extends Rectangulo{

            Constructor Cubo(int lado){
              this.ladoA=lado;
              this.ladoB=lado;
            }
}
int main(){
    Cuadrado c = new Cuadrado();
    c.perimetro();
    c.area();
}