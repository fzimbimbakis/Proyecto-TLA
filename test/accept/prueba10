Class Prueba{


    Constructor Prueba(){}

    int equal(int a, int b){
        return a==b;
    }

    int notEqual(int a , int b){
        return a!=b;
    }

     int lower(int a, int b){
        return a<b;
    }

     int greater(int a, int b){
        return a>b;
    }

    int lowerEq(int a, int b){
        return a<=b;
    }

    int greaterEq(int a, int b){
        return a>=b;
    }

    int add(int a, int b){
        return a+b;
    }

    int substract(int a, int b){
        return a-b;
    }

    int mult(int a, int b){
        return a*b;
    }

    int division(int a, int b){
        return a/b;
    }

    int and(int a, int b){
        return a&&b;
    }

    int or(int a, int b){
        return a||b;
    }

}

int main(){
       Prueba r= new Prueba();
       printf("%d\n",r.or(0,2));
       printf("%d\n",r.and(0,2));
       printf("%d\n",r.mult(0,2));
       printf("%d\n",r.division(0,2));
       printf("%d\n",r.add(0,2));
       printf("%d\n",r.substract(0,2));
       printf("%d\n",r.lower(0,2));
       printf("%d\n",r.lowerEq(0,2));
       printf("%d\n",r.greater(0,2));
       printf("%d\n",r.greaterEq(0,2));
       printf("%d\n",r.equal(0,2));
    }