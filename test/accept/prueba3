Class Prueba{

    Attributes {
        int a;
        int b;
    }

    Constructor Prueba(int a, int b){
      this.a=a;
      this.b=b;
    }


    int suma () {
       return this.a + this.b;
    }
}
int main(){
       Prueba prueba =new Prueba(1, 2);
       int sumando = prueba.suma();
       printf("%d\n",sumando);
       int integer = 1;
       printf("%d\n",integer);
       char character = 'a';
        printf("%c\n",character);
       char string[] = "String";
       printf("%s\n",string);
       int integerArray[] = {1, 2, 3, 4, 5};
    }