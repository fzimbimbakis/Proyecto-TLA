Class NumberList{

    Attributes{
        int list[255];
        int count;
    }
    Constructor NumberList(){
        this.count = 0;
    }

    int read(){
        int total = atoi(getchar());
        int n;
        while( this.count!=255 && total != 0){
            this.list[this.count] = atoi(getchar());
            this.count = this.count+1;
            total = total-1;
        }
    }

    int modify(int n){
        int i = 0;
        while ( i < this.count) {
            this.list[i] =  this.list[i] * n ;
            i = i + 1;
        }
    }

    int write(){
        int i = 0;
        while ( i < this.count) {
            printf("%d", this.list[i]);
            i = i + 1;
        }
    }
}

int main() {
    NumberList list = new NumberList();
    list.read();
    list.modify(2);
    list.write();
}
