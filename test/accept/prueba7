Class NumberList{

    Attributes{
        int list[255];
        int count;
    }
    Constructor NumberList(){
        this.count = 0;
    }

    int read(){
        int total = atoi(scanf());
        int n;
        while( count!=255 && total != 0){
            list[count] = atoi(scanf());
            count = count+1;
            total = total-1;
        }
    }

    int modify(int n){
        int i = 0;
        while ( i < count) {
            list[i] = list[i] * n;
            i = i + 1;
        }
    }

    int write(){
        int i = 0;
        while ( i < count) {
            printf("%d", list[i]);
            i = i + 1;
        }
    }
}

int main() {
    NumberList list = new NumberList();
    list.read();
    list.modify(2);
    list.write();
}
