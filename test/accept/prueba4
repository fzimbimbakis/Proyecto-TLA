Class Fibonacci{

    Attributes {
        int a;
        int b;
    }

    Constructor Fibonacci(){
      this.a=1;
      this.b=0;
    }


    int next () {
     int aux = this.a;
     this.b = this.a;
     this.a = this.a + this.b;
     return aux;
    }

}

int main(){
       Fibonacci f= new Fibonacci();
       printf("%d\n",f.a);
       f.next();
        printf("%d\n",f.a);
        f.next();
        printf("%d\n",f.a);

}