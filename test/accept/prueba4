Class Fibonacci{

    Attributes {
        int a;
        int b;
    }

    Constructor Fibonacci(){
      this.a=1;
      this.b=0;
    }


    int next () {
     int aux = a;
     b = a;
     a = a + b;
     return aux;
    }

}

int main(){
       Fibonacci f= new Fibonacci();
       f.next();
       f.next();
}