Class Prueba{

    Attributes {
        int a;
        int b;
    }

    Constructor Prueba(int a, int b){
      this.a=a;
      this.b=b;
    }


    int suma () {
       return this.a + this.b;
    }
}
int main(){
       Prueba r= new Prueba(3,2);
        r.suma();
        int integer = r.a;
        printf("a = %d\n",integer);
        printf("b = %d\n",r.b);
        printf("a + b = %d\n",r.suma());
    }
