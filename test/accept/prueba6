Class Persona{
    Attributes {
            int dni;
            char nombre[];
        }

        Constructor Persona(int dni, char nombre[]){
          this.dni = dni;
          this.nombre = nombre;
        }


}
Class ListaPersonas{
    Attributes {
            Persona personas[300];
            int n;
            int capacity;
        }

        Constructor ListaPersonas(int capacity){
           this.n = 0;
           this.capacity = capacity;
        }

        int compare(char a[], char b[])
        {
            int i=0;
            while(a[i]!=0 &&b[i]!=0)
            {
               if(a[i]!=b[i])
               {
                   return a[i]-b[i];
               }
               i=i+1;
            }
            return 0;
        }

        int registrar(int dni, char nombre[])
        {
            if (this.n >= this.capacity && this.n > 300){
                return this.n;
                }

            int i = this.n - 1;
            i--;
            i++;
            int comp;
            if(this.n != 0){
                comp = this.compare(this.personas[i].nombre, nombre);
            }
            while(i >= 0 && (comp<0 || (comp==0 && this.personas[i].dni > dni) )){
                this.personas[i + 1] = this.personas[i];
                i = i - 1;
                if(i >= 0){
                comp = this.compare(this.personas[i].nombre, nombre);
                }
                }

            this.personas[i + 1] = new Persona(dni, nombre);

            return (this.n + 1);
        }


    }

int main(){
    ListaPersonas lista = new ListaPersonas(10);
    lista.registrar(1, "A");
    printf("%d\n", lista.personas[0].dni);
    lista.registrar(2, "A");
    printf("%d\n",lista.personas[1].dni);
    lista.registrar(3, "B");
    printf("%d\n", lista.personas[2].dni);
     lista.registrar(4, "C");
     printf("%d\n", lista.personas[3].dni);
     lista.registrar(5, "C");
     printf("%d\n", lista.personas[4].dni);


}
