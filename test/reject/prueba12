Class Fibonacci{

    Attributes {
            int a;
            int b;
        }

        Constructor Fibonacci(){
          this.a=1;
          this.b=0;
        }


        int next () {
         int aux = a;
         b = a;
         a = a + b;
         return aux;
        }


}


Class Prueba{

    Attributes {
        int a;
        int b;
    }

    Constructor Prueba(int a, int b){
      this.a=a;
      this.b=b;
    }
}

int main(){
       Prueba r= new Prueba(3,2);
       Fibonacci f=new Fibonacci();

       r=f;

    }