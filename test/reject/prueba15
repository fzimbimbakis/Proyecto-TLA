Class A{
    Attributes {
            int a;
        }

        Constructor A(int a){
          this.a=a;
        }
}
Class B{
    Attributes {
            int b;
        }

        Constructor B(int b){
          this.b=b;
        }


        int suma () {
           return this.a + this.b;
        }

}
