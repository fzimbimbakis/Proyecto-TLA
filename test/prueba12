Class Fibonacci{

    Attributes {
        int actual;
    }

    Constructor Fibonacci(){
      this.actual=0;
    }

    int fibo(int n){

        if(n==0)
            return 0;
        else if(n==1)
            return 1;
        else
            return fibo(n-2)+fibo(n-1);

    }


    int next () {
        int a=actual;
        actual=actual+1;
       return fibo(a);
    }

}


Class Prueba{

    Attributes {
        int a;
        int b;
    }

    Constructor Prueba(int a, int b){
      this.a=a;
      this.b=b;
    }
}

int main(){
       Prueba r= new Prueba(3,2);
       Fibonacci f=new Fibonacci();

       r=f;

    }