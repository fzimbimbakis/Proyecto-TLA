class Rectangulo{
    attributes {
            int lado;
        }

        constructor Cuadrado(int lado){
          this.lado = lado;
        }


    int area () {
           return this.lado * 2;
        }
    int perimetro(){
           return this.lado * 4;
        }
}
class Cubo extends Cuadrado{
    attributes {
            }

            constructor Cubo(int lado){
              this.lado=lado;
            }

        int area() {
            return lado * 2 * 6;
        }

        int sumaAristas(){
            return this.perimetro() * 3;
        }
}
